<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
  <cron>
    <url>/tasks/mailSubscriptions</url>
    <description>Mail Report Subscriptions</description>
    <schedule>every day 00:05</schedule>
  </cron>

  <cron>
    <url>/tasks/activitydigests</url>
    <description>Mail Weekly Activity Digests</description>
    <schedule>every saturday 17:00</schedule>
  </cron>

  <cron>
    <url>/tasks/geodigests</url>
    <description>Mail Daily Geographic Digests</description>
    <schedule>every day 17:00</schedule>
  </cron>

  <cron>
    <url>/bqlogging/logExportDirectToBigqueryStart</url>
    <description>Export logs to BigQuery</description>
    <schedule>every 1 minutes</schedule>
    <target>logging</target>
  </cron>
</cronentries>