<module rename-to="App">

    <inherits name='org.activityinfo.ui.App'/>

    <!-- Minimize CSS names -->
    <set-configuration-property name="CssResource.style" value="obf"/>

    <!-- Turn off logging -->
    <!--<set-property name="gwt.logging.enabled" value="TRUE"/>-->

    <!-- Limit to supported browsers -->
    <set-property name="gxt.user.agent" value="safari5,chrome,ie9,ie10,ie11,gecko1_9"/>

    <!-- Generate AppCache manifests -->
    <define-linker name="std" class="com.bedatadriven.rebar.appcache.linker.AppCacheIFrameLinker"/>
    <add-linker name="std"/>



</module>
