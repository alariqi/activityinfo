

body {
    font-family: 'Overpass', sans-serif;
    line-height: 1.3;
}

svg:not(:root) {
    display: block;
    overflow: hidden;
    fill: currentColor;
}

/* Custom scrollbars */

::-webkit-scrollbar{
    height:8px;
    width:8px
}
::-webkit-scrollbar-track{
    background-color:rgba(101,116,127,.1)
}
::-webkit-scrollbar-thumb{
    background-color:#65747f
}
